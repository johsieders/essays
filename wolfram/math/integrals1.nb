 RowBox[{
  RowBox[{"\[Phi]", " ", "+", " ", "\[Rho]"}], "<=", 
  RowBox[{"f", "+", "g"}], "<=", 
  RowBox[{
  "\[Psi]", " ", "+", " ", "\[Sigma]", " "}]}]], "DisplayFormulaNumbered",
