Cell[BoxData[
 RowBox[{
  RowBox[{"\[Omega]", "\[Wedge]", "\[Eta]"}], "=", 
  RowBox[{
   RowBox[{
    FractionBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{"k", "+", "l"}], ")"}], "!"}], 
     RowBox[{
      RowBox[{"k", "!"}], 
      RowBox[{"l", "!"}]}]], 
    RowBox[{"alt", "[", 
     RowBox[{"\[Omega]", "\[TensorProduct]", "\[Eta]"}], "]"}]}], "=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{
      RowBox[{"k", "!"}], 
      RowBox[{"l", "!"}]}]], 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"\[Sigma]", "\[Element]", 
       SubscriptBox["Perm", "k"]}], "n"], 
     RowBox[{
      RowBox[{"sgn", "[", "\[Sigma]", "]"}], 
      RowBox[{"\[Omega]", "\[TensorProduct]", 
       RowBox[{"\[Eta]", "[", 
        RowBox[{"\[Sigma]", "[", 
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["x", 
            RowBox[{"(", "1", ")"}]], ",", "..", ",", 
           SuperscriptBox["x", 
            RowBox[{"(", "k", ")"}]], ",", 
           SuperscriptBox["x", 
            RowBox[{"(", 
             RowBox[{"k", "+", "1"}], ")"}]], ",", "..", ",", 
           SuperscriptBox["x", 
            RowBox[{"(", 
             RowBox[{"k", "+", "l"}], ")"}]]}], " ", ")"}], "]"}], 
        "]"}]}]}]}]}]}]}]], "DisplayFormulaNumbered",
