   Cell["(n,l)",ExpressionUUID->"78be84e7-c447-eb4e-bf74-98abb725490f"], ".", 
    "The"}], " ", "wedge", " ", "operator", " ", "is", " ", "defined", " ", 
   "as"}]}]], "Text",
