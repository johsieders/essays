 Cell[BoxData[
  FormBox[
   RowBox[{"\[Rho]", "<=", "g", "<=", "\[Sigma]"}], TraditionalForm]],
  ExpressionUUID->"62d0ed08-bb5c-b340-a81d-4b8c41a0630c"],
 ". So"
}], "Text",
