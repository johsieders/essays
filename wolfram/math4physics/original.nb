 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"[", 
     RowBox[{"a", ",", "b"}], "]"}], " "}], TraditionalForm]],ExpressionUUID->
  "00aa433d-e746-5b4b-bea3-efbcb274d274"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["min", 
     RowBox[{
      RowBox[{"k", " ", "=", "1"}], ",", "..", ",", "n"}]], "  ", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["x", "k"], "-", 
      SubscriptBox["x", 
       RowBox[{"k", "-", "1"}]]}], "}"}]}], TraditionalForm]],ExpressionUUID->
  "382d454a-9b5b-7b46-b6f6-c75eb1c44412"],
 " its granularity, and ",
 Cell[BoxData[
  RowBox[{"{", 
   RowBox[{
    SubscriptBox["\[Xi]", "1"], ",", " ", 
    SubscriptBox["\[Xi]", "2"], ",", " ", "..", " ", ",", 
    SubscriptBox["\[Xi]", "n"]}], "}"}]],
